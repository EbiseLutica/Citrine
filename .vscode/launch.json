{
	// Use IntelliSense to find out which attributes exist for C# debugging
	// Use hover for the description of the existing attributes
	// For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Core Launch (Misskey)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build-misskey",
			// If you have changed target frameworks, make sure to update the program path.
			"program": "${workspaceFolder}/Citrine.Misskey/bin/Debug/netcoreapp2.1/Citrine.Misskey.dll",
			"args": [],
			"cwd": "${workspaceFolder}/Citrine.Misskey/bin/Debug/netcoreapp2.1/",
			// For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
			"console": "integratedTerminal",
			"stopAtEntry": false,
			"internalConsoleOptions": "openOnSessionStart"
		},
		{
			"name": ".NET Core Launch (Mastodon)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build-mastodon",
			// If you have changed target frameworks, make sure to update the program path.
			"program": "${workspaceFolder}/Citrine.Mastodon/bin/Debug/netcoreapp2.1/Citrine.Mastodon.dll",
			"args": [],
			"cwd": "${workspaceFolder}/Citrine.Mastodon/bin/Debug/netcoreapp2.1/",
			// For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
			"console": "integratedTerminal",
			"stopAtEntry": false,
			"internalConsoleOptions": "openOnSessionStart"
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach",
			"processId": "${command:pickProcess}"
		}
	,]
}
